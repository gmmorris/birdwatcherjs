{
  "name": "birdwatcher",
  "description": "A covert error handling utility for Javascript. A small utility for implementing the NCZ JavaScript error handling pattern",
  "version": "1.0.21",
  "author": "Gidi Meir Morris",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gmmorris/birdwatcherjs.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/gmmorris/birdwatcherjs/blob/master/LICENSE"
    }
  ],
  "scripts": {
    "lint": "eslint src test",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf lib",
    "test": "mocha --compilers js:babel/register --require ./test/utils/tools.js --recursive",
    "test:watch": "npm run test -- --watch",
    "test:cov": "babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --recursive"
  },
  "dependencies": {},
  "devDependencies": {
    "babel": "^5.8.29",
    "babel-eslint": "^4.1.3",
    "chai": "^3.2.0",
    "eslint": "^1.7.3",
    "eslint-config-airbnb": "^0.1.0",
    "eslint-plugin-react": "^3.6.3",
    "isparta": "^3.1.0",
    "mocha": "^2.3.2",
    "mocha-lcov-reporter": "0.0.1",
    "rimraf": "^2.4.3",
    "simple-assert": "^1.0.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0"
  },
  "keywords": [
    "Error",
    "Birdwatcher.js"
  ]
}
